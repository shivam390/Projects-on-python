from selenium import webdriver
from selenium.webdriver.chrome.options import Options
import os
import time



#current directory
chrome_options = Options()
chrome_options.add_argument('headless')
chrome_options.add_argument("--window-size=1920x1080")
chrome_driver = os.getcwd()+"\\chromedriver.exe"
driver = webdriver.Chrome(chrome_options=chrome_options, executable_path=chrome_driver)
driver.get("Enter weburl")
button = driver.find_element_by_xpath("""//*[Username xpath @id="user"]""")
button.click()
button.send_keys("Username")
button = driver.find_element_by_xpath("""//*[Password xpath @id="password"]""")
button.click()
button.send_keys("Password")
button = driver.find_element_by_xpath("""Xpath from website for submit""")
button.click()
time.sleep(10)
#working with table and rows and columns to get the data and then save the data to particular file in computer
driver.find_element_by_link_text('''Inbox''').click()
for row in driver.find_elements_by_css_selector('''"tr.class="tablePair""'''):
    cell = row.find_elements_by_tag_name("td")
    print(cell.text)
    
